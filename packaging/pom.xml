    <groupId>org.agmip.ui.workbench.modules</groupId>
    <artifactId>ria-cropmodel-packaging</artifactId>
    <version>1.0.3</version>
    <packaging>nbm</packaging>
    <name>AgMIP Workbench RIA Crop Model Packaging Module</name>

    <build>
        <plugins>
            <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>nbm-maven-plugin</artifactId>
